APP_NAME=CorralX
APP_ENV=production
APP_KEY=
APP_DEBUG=false
APP_URL=https://backend.corralx.com
APP_URL_LOCAL=http://localhost:8000
APP_URL_PRODUCTION=https://backend.corralx.com

LOG_CHANNEL=stack
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=
DB_USERNAME=
DB_PASSWORD=

# Broadcasting
BROADCAST_DRIVER=pusher
PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=
PUSHER_HOST=
PUSHER_PORT=443
PUSHER_SCHEME=https

# Cache
CACHE_DRIVER=file
SESSION_DRIVER=file
SESSION_LIFETIME=120
QUEUE_CONNECTION=sync

# Redis (opcional)
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

# Memcached (opcional)
MEMCACHED_HOST=127.0.0.1

# Filesystem
FILESYSTEM_DISK=local

# AWS S3 (opcional)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# Mail
MAIL_MAILER=smtp
MAIL_HOST=
MAIL_PORT=587
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=tls
MAIL_FROM_ADDRESS=noreply@corralx.com
MAIL_FROM_NAME="${APP_NAME}"

# Firebase (FCM - Notificaciones Push)
FIREBASE_CREDENTIALS=storage/app/corralx-777-aipp-firebase-adminsdk-*.json
FIREBASE_DATABASE_URL=https://corralx-777-aipp-default-rtdb.firebaseio.com
FIREBASE_STORAGE_BUCKET=corralx-777-aipp.firebasestorage.app

# Google Gemini AI
GOOGLE_GEN_AI_KEY=
GOOGLE_GEN_AI_BASE_URL=https://generativelanguage.googleapis.com/v1beta
GOOGLE_GEN_AI_MODEL=models/gemini-2.0-flash

# Sanctum
SANCTUM_STATEFUL_DOMAINS=localhost,127.0.0.1,127.0.0.1:8000,::1,backend.corralx.com

# CORS
CORS_ALLOWED_ORIGINS=https://corralx.com,https://www.corralx.com

# Vite (Frontend)
VITE_APP_NAME="${APP_NAME}"
VITE_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
VITE_PUSHER_HOST="${PUSHER_HOST}"
VITE_PUSHER_PORT="${PUSHER_PORT}"
VITE_PUSHER_SCHEME="${PUSHER_SCHEME}"
VITE_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"
